{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "classes": true
        }
    },

    "plugins": [
      "react"
    ],
    "rules": {
      "react/display-name": 1
    }
}